@model System.Collections.Generic.IDictionary<string, Models.RequestData>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Requests Stats</title>
    <style>
        table {        
            width: 600px;
            margin: 0 auto;
            border: 1px solid black;
        }
        td {
            border: 1px solid black; 
            text-align: center;
            padding-left: 5px;
        }
    </style>
</head>
<body>
<div>
    <table>
        <tr>
            <th>URL</th>
            <th>Amount</th>
            <th>Last Request</th>
        </tr>
        @foreach (var key in Model.Keys)
        {
            <tr>
                <td>@key</td>
                <td>@Model[key].Amount</td>
                <td>@Model[key].LastRequest</td>
            </tr>
        }
    </table>
</div>
</body>
</html>